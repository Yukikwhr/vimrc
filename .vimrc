"disp line number
set number

"disp pair brackets
set showmatch

"disp syntax highlight
syntax enable

"disp search result highlight
set hlsearch

"incremental search
set incsearch

"beep sound off
set visualbell t_vb=

"complement file name
set wildmode=longest,full

"backspacekey on
set backspace=indent,eol,start

"clipboard on
"cui
"set clipboard+=autoselect
"gui
set guioptions+=a
set clipboard+=unnamed

